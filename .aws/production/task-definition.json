{
    "containerDefinitions": [
        {
            "name": "filestash",
            "image": "006759544100.dkr.ecr.us-west-1.amazonaws.com/filestash:latest",
            "cpu": 1024,
            "memoryReservation": 2048,
            "portMappings": [
                {
                    "containerPort": 8334,
                    "hostPort": 8334,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [
                {
                    "name": "AWS_DEFAULT_REGION",
                    "value": "us-west-1"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "app-data",
                    "containerPath": "/mnt/efs"
                }
            ],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/bgmc-youtube-prod-filestash",
                    "awslogs-region": "us-west-1",
                    "awslogs-stream-prefix": "filestash"
                }
            }
        }
    ],
    "family": "bgmc-youtube-prod-filestash-ecs-task",
    "taskRoleArn": "arn:aws:iam::006759544100:role/bgmc-youtube-filestash-ecs-task-service-role",
    "executionRoleArn": "arn:aws:iam::006759544100:role/bgmc-youtube-filestash-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "app-data",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-01463bf051aac74d0",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-043b6565d685add63",
                    "iam": "ENABLED"
                }
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}
